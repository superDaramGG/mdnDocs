function work(callback) {
  setTimeout(() => {
    const start = Date.now();
    for(let i = 0; i < 1000000000; i++) {

    }
    const end = Date.now();
    console.log(end - start + 'ms');
    callback(end - start)
  }, 0)
}

work((ms) => {
  console.log('작업이 끝났어요!');
  console.log(ms + 'ms 걸렸다고 해요.');
});
console.log('다음 작업')

//보통의 작업은 work가 진행되고, 콘솔이 찍히는데
//work()가 진행되는 동안에 다른 일도 하고 싶다 하면 setTimeOut
