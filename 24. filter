const todos = [
  {
    id:1,
    text:'자바스크립트 입문',
    done: true,
  },
  {
    id:2,
    text:'함수 배우기',
    done: true,
  },
  {
    id:3,
    text:'객체와 배열 배우기',
    done: true,
  },
  {
    id:4,
    text:'배열 내장함수 배우기',
    done: false,
  }      
]

//특정 조건 원소들만 추출할 때 사용한다.
const tasksNotDone = todos.filter(todo => doto.done === false);
console.log(tasksNotDone);
