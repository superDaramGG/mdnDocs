//배열을 다룰때 for...of
const numbers = [10, 20, 30, 40, 50];

for(let number of numbers) {
  console.log(number);
}

//for...in 객체를 받아올떄
const doggy = {
  name: '멍멍이',
  sound: '멍멍',
  age: 2  
}

console.log(Object.entries(doggy));
console.log(Object.keys(doggy));
console.log(Object.values(doggy));

const doggy = {
  name: '멍멍이',
  sound: '멍멍',
  age: 2  
}

for(let key in doggy) {
  console.log(`${key}: ${doggy[key]}`);
}
